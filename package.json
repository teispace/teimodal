{
  "name": "@teispace/teimodal",
  "version": "1.0.1",
  "description": "A beautiful, customizable React modal component",
  "author": "Teispace",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "react",
    "modal",
    "dialog",
    "typescript",
    "tailwindcss",
    "ui",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teispace/teimodal.git"
  },
  "bugs": {
    "url": "https://github.com/teispace/teimodal/issues"
  },
  "homepage": "https://github.com/teispace/teimodal#readme",
  "contributors": [
    "Krishna Adhikari (https://github.com/iamkrishnaa)",
    "Teispace (https://github.com/teispace)"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist && rollup -c",
    "prepare": "husky",
    "test": "NODE_ENV=test jest --ci --runInBand --detectOpenHandles",
    "check-types": "tsc --pretty --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist && rm -rf coverage && rm -rf node_modules && npm install"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.28.0",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.1",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/supertest": "^6.0.3",
    "commitlint": "^19.8.1",
    "eslint": "^9.28.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "rollup": "^4.41.1",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "semantic-release": "^24.2.5",
    "supertest": "^7.1.1",
    "tailwindcss": "^4.1.8",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  }
}
